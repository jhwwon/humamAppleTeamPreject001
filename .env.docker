# MusicSpace Docker Environment Variables
# 이 파일을 .env로 복사하여 사용하세요: cp .env.docker .env

# Database
DB_ROOT_PASSWORD=musicspace123
DB_NAME=music_space_db
DB_USER=musicspace
DB_PASSWORD=musicspace123

# JWT Secret (변경 필수! openssl rand -base64 32)
JWT_SECRET=change_this_to_random_secret_key

# Tidal API
TIDAL_CLIENT_ID=your_tidal_client_id
TIDAL_CLIENT_SECRET=your_tidal_client_secret

# Spotify API
SPOTIFY_CLIENT_ID=your_spotify_client_id
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret

# YouTube API
YOUTUBE_KEY=your_youtube_api_key

# Last.fm API
LASTFM_API_KEY=your_lastfm_api_key
